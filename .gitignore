# This .gitignore file should be placed at the root of your Unity project directory
#
# Get latest from https://github.com/github/gitignore/blob/main/Unity.gitignore
#
/Game/[Ll]ibrary/
/Game/[Tt]emp/
/Game/[Oo]bj/
/Game/[Bb]uild/
/Game/[Bb]uilds/
/Game/[Ll]ogs/
/Game/[Uu]ser[Ss]ettings/

# MemoryCaptures can get excessive in size.
# They also could contain extremely sensitive data
/Game/[Mm]emoryCaptures/

# Recordings can get excessive in size
/Game/[Rr]ecordings/

# Uncomment this line if you wish to ignore the asset store tools plugin
# /Game/[Aa]ssets/AssetStoreTools*

# Autogenerated Jetbrains Rider plugin
/Game/[Aa]ssets/Plugins/Editor/JetBrains*

# Visual Studio cache directory
/Game/.vs/

# Gradle cache directory
/Game/.gradle/

# Autogenerated VS/MD/Consulo solution and project files
ExportedObj/
/Game/.consulo/
/Game/*.csproj
/Game/*.unityproj
/Game/*.sln
/Game/*.suo
/Game/*.tmp
/Game/*.user
/Game/*.userprefs
/Game/*.pidb
/Game/*.booproj
/Game/*.svd
/Game/*.pdb
/Game/*.mdb
/Game/*.opendb
/Game/*.VC.db

# Unity3D generated meta files
/Game/*.pidb.meta
/Game/*.pdb.meta
/Game/*.mdb.meta

# Unity3D generated file on crash reports
/Game/sysinfo.txt

# Builds
/Game/*.apk
/Game/*.aab
/Game/*.unitypackage
/Game/*.app

# Crashlytics generated file
/Game/crashlytics-build.properties

# Packed Addressables
/Game/[Aa]ssets/[Aa]ddressable[Aa]ssets[Dd]ata/*/*.bin*

# Temporary auto-generated Android Assets
/Game/[Aa]ssets/[Ss]treamingAssets/aa.meta
/Game/[Aa]ssets/[Ss]treamingAssets/aa/*

